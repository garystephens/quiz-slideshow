{
    "name": "quiz-slideshow",
    "version": "1.0.0",
    "description": "Present quiz questions and answers as a slideshow",
    "main": "main.js",
    "scripts": {
        "dev": "rmdir /s /q dist && webpack --mode development",
        "build": "webpack --mode production",
        "start": "cross-env NODE_ENV=dev webpack-dev-server --mode development --env development --open",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist",
        "eslint": "eslint . --format visualstudio"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/garystephens/quiz-slideshow.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/garystephens/quiz-slideshow/issues"
    },
    "homepage": "https://garystephens.github.io/quiz-slideshow/",
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@babel/preset-env": "^7.15.6",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.15.0",
        "@types/jquery": "^3.5.6",
        "@types/react-dom": "^17.0.9",
        "@types/styled-components": "^5.1.14",
        "@typescript-eslint/eslint-plugin": "^4.31.2",
        "@typescript-eslint/parser": "^4.31.2",
        "babel-loader": "^8.2.2",
        "babel-plugin-styled-components": "^1.13.2",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^7.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^5.2.7",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-react": "^7.26.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "gh-pages": "^3.2.3",
        "html-webpack-plugin": "^4.5.2",
        "normalize.css": "^8.0.1",
        "prettier": "^2.4.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-use-keypress": "^1.3.0",
        "style-loader": "^2.0.0",
        "styled-components": "^5.3.1",
        "ts-loader": "^9.2.6",
